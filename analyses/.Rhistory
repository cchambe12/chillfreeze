bbdat <- read.csv("output/clean_dvr_traits.csv")
bbdat$chillnew <- ifelse(bbdat$chill==1, 3, NA)
bbdat$chillnew <- ifelse(bbdat$chill==2, 2, bbdat$chillnew)
bbdat$chillnew <- ifelse(bbdat$chill==3, 1, bbdat$chillnew)
bbdat$chill1 = ifelse(bbdat$chillnew == 2, 1, 0)
bbdat$chill2 = ifelse(bbdat$chillnew == 3, 1, 0)
bb.mod <- lm(budburst ~ chill1 + chill2, data=bbdat)
simpbbchill1 <- format(summary(bb.mod)$coef[2,1],  nsmall=1)
simpbbchillsd1 <- format(summary(bb.mod)$coef[2,2],  nsmall=1)
simpbbchill2 <- format(summary(bb.mod)$coef[3,1],  nsmall=1)
simpbbchillsd2 <- format(summary(bb.mod)$coef[3,2],  nsmall=1)
dvr.mod <- lm(dvr ~ chill1 + chill2, data=bbdat)
simpdvrchill1 <- format(summary(dvr.mod)$coef[2,1],  nsmall=1)
simpdvrchillsd1 <- format(summary(dvr.mod)$coef[2,2],  nsmall=1)
